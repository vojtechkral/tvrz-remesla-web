{
  "name": "redux-starter",
  "version": "2.0.0",
  "description": "Personal starter pack for redux applications.",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomvej/redux-starter.git"
  },
  "scripts": {
    "start": "webpack-dev-server --progress --mode development --env.devserver",
    "prebuild": "run-s lint test clean",
    "clean": "rimraf dist/*",
    "build": "webpack --progress --mode production --env",
    "lint": "eslint src *.js",
    "lint:fix": "npm run lint -- --fix",
    "test:base": "mocha --require \"@babel/register\" test.config.spec.js \"src/**/*.spec.js\"",
    "test": "npm run test:base -- --reporter spec",
    "test:watch": "npm run test:base -- --reporter min --watch"
  },
  "author": "tom.vejpustek@centrum.cz",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "4.3.1",
    "file-loader": "^3.0.1",
    "prop-types": "15.7.2",
    "ramda": "0.26.1",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-redux": "5.1.1",
    "redux": "4.0.1",
    "redux-freeze": "^0.1.7",
    "redux-saga": "1.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-react-constant-elements": "7.2.0",
    "@babel/plugin-transform-runtime": "7.4.0",
    "@babel/polyfill": "7.4.0",
    "@babel/preset-env": "7.4.1",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.0",
    "@babel/runtime": "7.4.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "chai": "4.2.0",
    "circular-dependency-plugin": "5.0.2",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "dirty-chai": "2.0.1",
    "eslint": "5.15.3",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "1.5.1",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.5.0",
    "mocha": "6.0.2",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.3",
    "sass": "1.17.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "2.1.2",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1"
  }
}
